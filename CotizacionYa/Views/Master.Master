<%@ Master Language="C#" AutoEventWireup="true" CodeBehind="Master.master.cs" Inherits="CotizacionYa.Views.Master" %>

<!DOCTYPE html>
<html class="no-js" lang="es-ar">
<head runat="server">
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>COTIZACION YA</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <!-- Favicon -->
    <asp:PlaceHolder runat="server">
        <!-- Bootstrap core CSS -->
        <link rel="stylesheet" href="<%= ResolveClientUrl("../../assets/plugins/bootstrap/css/bootstrap.min.css")%>">
